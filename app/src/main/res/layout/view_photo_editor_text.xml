<esaph.spotlight.Esaph.EsaphSpotLightPhotoEditor.EditorPack.EsaphConsumerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:clickable="true"
    android:focusable="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <FrameLayout
        android:background="@null"
        android:id="@+id/frmBorder"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <esaph.spotlight.spots.SpotMaker.SpotMakerView.Objects.SpotMakerEdittext
            android:textCursorDrawable="@null"
            android:background="@null"
            android:id="@+id/tvPhotoEditorText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#000000"
            android:textSize="40sp" />

    </FrameLayout>

</esaph.spotlight.Esaph.EsaphSpotLightPhotoEditor.EditorPack.EsaphConsumerLayout>
